                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        Appointment.*, Dentist.*, Drug.*, Patient.*, Prescription.*, Payment.*, Receptionist.*
FROM            Appointment INNER JOIN
                         Dentist ON Appointment.app_id = Dentist.dentist_id INNER JOIN
                         Patient ON Appointment.app_id = Patient.patient_id INNER JOIN
                         Payment ON Appointment.app_id = Payment.payment_id AND Patient.patient_id = Payment.payment_id INNER JOIN
                         Prescription ON Dentist.dentist_id = Prescription.prescp_no AND Patient.patient_id = Prescription.prescp_no INNER JOIN
                         Drug ON Prescription.prescp_no = Drug.prescp_no INNER JOIN
                         Receptionist ON Appointment.app_id = Receptionist.recep_id</CommandText>
